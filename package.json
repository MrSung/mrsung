{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <oi@joaopedro.cc> (joaopedro.cc)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:no-cache": "run-s clean build",
    "start": "next start",
    "clean": "rimraf .next",
    "tsc": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "stylelint": "stylelint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx|scss|css)\" --write",
    "postinstall": "typesync",
    "storybook": "start-storybook -p 7007",
    "storybook:no-cache": "start-storybook --no-manager-cache -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "isomorphic-unfetch": "3.1.0",
    "next": "10.2.0",
    "next-themes": "0.0.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.16.0",
    "sass": "1.32.13"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@tailwindcss/postcss7-compat": "2.1.2",
    "@types/autoprefixer": "^10.2.0",
    "@types/babel__core": "^7.1.14",
    "@types/eslint": "7.2.10",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/node": "15.0.3",
    "@types/prettier": "2.2.3",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "@types/rimraf": "3.0.0",
    "@types/sass": "1.16.0",
    "@types/stylelint": "9.10.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.26.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "^7",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-property-sort-order-smacss": "7.1.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-order": "4.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "4.2.4",
    "typesync": "0.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
